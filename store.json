      "x-casaos": {
        "architectures": [
          "amd64"
        ],
        "main": "docker",
        "docker": {
          "image": "ghcr.io/joshevans-weber/toontown-realms-server:latest",
          "container_name": "toontown-realms-server",
          "restart_policy": "unless-stopped",
          "network_mode": "bridge",
          "ports": [
            "7198:7198/tcp",
            "7199:7199/tcp",
            "7197:7197/tcp"
          ],
          "volumes": [
            {
              "source": "/DATA/AppData/toontown-realms/astron/databases",
              "target": "/app/astron/databases",
              "bind": {}
            },
            {
              "source": "/DATA/AppData/toontown-realms/config",
              "target": "/app/config",
              "bind": {}
            },
            {
              "source": "/DATA/AppData/toontown-realms/logs",
              "target": "/app/logs",
              "bind": {}
            }
          ],
          "environment": [
            "TZ=UTC",
            "PUID=1000",
            "PGID=1000"
          ]
        }
      }
    }
